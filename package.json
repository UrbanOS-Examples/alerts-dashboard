{
    "name": "alerts-api",
    "version": "0.0.2",
    "description": "API for the alerts dashboard",
    "main": "src/start.js",
    "bin": "src/start.js",
    "scripts": {
        "start": "node src/start.js",
        "build": "tsc",
        "watch": "nodemon src/start.ts",
        "test": "jest --coverage",
        "view:cov": "serve coverage/lcov-report",
        "clean": "rm -rf coverage src/**/*.js src/**/*.map",
        "lint": "eslint '*/**/*.ts'",
        "lint:fix": "eslint '*/**/*.ts' --fix",
        "test:watch": "jest --coverage --watchAll",
        "view:coverage": "serve coverage/lcov-report",
        "detect": "jest --runInBand --detectOpenHandles",
        "locations": "npx ts-node src/scripts/generateLocations.ts"
    },
    "author": "UrbanOS",
    "dependencies": {
        "@turf/turf": "^6.5.0",
        "@types/express": "^4.17.13",
        "@types/jest": "^25.2.3",
        "@types/node": "^14.17.15",
        "@types/redis": "^2.8.32",
        "@types/source-map-support": "^0.5.4",
        "@types/supertest": "^2.0.11",
        "@types/uuid": "^8.3.1",
        "@types/ws": "^8.2.0",
        "@typescript-eslint/eslint-plugin": "^3.10.1",
        "@typescript-eslint/parser": "^3.10.1",
        "express": "^4.17.1",
        "luxon": "^2.2.0",
        "redis": "^3.1.2",
        "source-map-support": "^0.5.20",
        "typescript": "^3.9.10",
        "uuid": "^8.3.2",
        "ws": "^8.2.2"
    },
    "devDependencies": {
        "@types/luxon": "^2.0.8",
        "@types/mock-fs": "^4.13.1",
        "@types/redis-mock": "^0.17.0",
        "async-wait-until": "^2.0.8",
        "eslint": "^7.32.0",
        "eslint-config-prettier": "^6.15.0",
        "eslint-plugin-prettier": "^3.4.1",
        "jest": "^26.6.3",
        "jest-junit": "^13.0.0",
        "mock-fs": "^5.1.1",
        "nodemon": "^2.0.12",
        "prettier": "^2.4.0",
        "redis-mock": "^0.56.3",
        "serve": "^13.0.2",
        "supertest": "^6.1.6",
        "ts-jest": "^26.5.6",
        "ts-node": "^8.10.2",
        "wait-for-expect": "^3.0.2"
    }
